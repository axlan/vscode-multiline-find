{
    "name": "multiline-find-and-replace",
    "displayName": "Multiline Find and Replace",
    "description": "Multiline Find and Replace",
    "version": "0.0.1",
    "publisher": "axlan",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.setFind"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.setFind",
                "title": "Set Find"
            },
            {
                "command": "extension.findNext",
                "title": "Find Next"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "sprintf-js": "^1.0.3"
    }
}
